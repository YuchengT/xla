{
    "ExecutorRole": "arn:aws:iam::389009836860:role/BenchmarkingExecutorRole",
    "ResourceConfig": {
        "region_name": "us-west-2",
        "InstanceType": "p3.2xlarge",
        "ImageId": "ami-0f3ae2af3f140da52",
        "IamInstanceProfile": { "Name": "HopperBenchmarking" },
        "TagSpecifications": [
        {
            "ResourceType": "instance",
            "Tags": [
                { "Key": "Name", "Value": "test-cdk-infra" }
            ]
        }]
    },
    "InputDataConfig": [
        {
            "ChannelName": "dataset",
            "DataSource": {
            "FileSystemDataSource": {
                "FileSystemId": "fs-05f9f632a620cea3c",
                "DirectoryPath": "/dataset"
                }
            }
        }
    ],
    "AlgorithmSpecifications":
    {
        "TrainingImage" : "875423407011.dkr.ecr.us-west-2.amazonaws.com/pytorch-xla-ec2-training-container/hopper/main:ubuntu18.04-cuda11.1-cudnn8-torch1.10-xla1.10-vision0.11",
        "TrainingScripts" : "s3://bucket/path/to/script/directory"
    },
    "HyperParameters":
    {
        "models": "gpt2",
        "sequence_lengths": "128"
    },
    "Environment":
    {
        "GPU_NUM_DEVICES": "1",
        "SAGEMAKER_PROGRAM": "hopper/test/scripts/launch_benchmarks.py",
        "PYTHONDONTWRITEBYTECODE": "1",
        "PYTHONUNBUFFERED": "1",
        "PYTHONIOENCODING": "UTF-8",
        "LANG": "C.UTF-8",
        "LC_ALL": "C.UTF-8"
    },
    "CloudWatchLogGroupName": "Benchmarks/EC2/PyTorch"
}